package com.java.Array;

public class LinkedList 
{
	private Node start;
	private Node last;
	private int count;
	
	public void add(Object ele) // For adding element
	{
		if (start==null) 
		{
			start=new Node(ele);
			last=start;
		}
		new Node(ele);
	}
	
	class Node {
		Object element;
		Node next; // address
		Node(Object ele)
		{
			element=ele;
		}
	}

}