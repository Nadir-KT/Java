package com.java.GUI.AWT;

import java.awt.Canvas;
import java.awt.Frame;

public class Canv {
	//Canvas: Rectangular Area
	public Canv() {
		Frame f=new Frame("Canvas");
		Canvas c=new Canvas();
		f.add(c);
		f.setSize(400, 400);
		f.setLayout(null);
		f.setVisible(true);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new Canv();
	}

}
