package com.java.Binding;

public class Binding
{
	void test1()
	{
		System.out.println("test 1 in Binding");
	}
	static void test2()
	{
		System.out.println("static test 2 in Binding");
	}
}
class A extends Binding
{
	@Override
	void test1()
	{
		System.out.println("test 1 in A");
	}
	static void test2()
	{
		System.out.println("static test 2 in A");
	}
}
class B extends Binding
{
	@Override
	void test1()
	{
		System.out.println("test 1 in B");
	}
	static void test2()//static, final, private methods cannot be Overrided
	{
		System.out.println("static test 2 in B");
	}
}