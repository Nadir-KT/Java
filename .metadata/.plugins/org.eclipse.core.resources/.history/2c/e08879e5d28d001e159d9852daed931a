package com.java.PrintSystem;

public class SystemOut {

	public static void main(String[] args) 
	{
		System.out.println("java");
	}
public class System
{
	public static PrintStream out=new PrintStream();
	public class PrintStream
	{
		public void print()
		{
			//Printing within same line
		}
		public void println()
		{
			//Printing within next line
		}
		public void println(int args)
		{
			//Printing within next line integer argument
		}
	}
}
}