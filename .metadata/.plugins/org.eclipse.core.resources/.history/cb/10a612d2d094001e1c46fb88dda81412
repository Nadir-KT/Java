package com.java.GUI.Applet;
//javac Applets.java
//appletviewer Applets.java
import java.applet.Applet;
import java.awt.Graphics;
public class Applets extends Applet 
{
    public void paint(Graphics g)
    {
    	//public abstract void drawString(String str, int x, int y) //Draws the text given by the specified string, using thisgraphics context's current font and color. 
        g.drawString("Applet", 150, 150);
        //public void drawRect(int x, int y, int width, int height) //Draws the outline of the specified rectangle. 
        g.drawRect(50, 70, 150, 150);
        //public abstract void fillRect(int x, int y, int width, int height);
        g.fillRect(ALLBITS, ABORT, WIDTH, HEIGHT);
    }
}
/*<html>
<body>
<applet code="Applets.class" width="300" height="300">
</applet>
</body>
</html>*/